lib	sysgetcwd note{ syscall(SYS_getcwd,buf,len) implemented }end link{
	#include <sys/syscall.h>
	int main()
	{
		char	buf[256];
		return syscall(SYS_getcwd, buf, sizeof(buf)) < 0;
	}
}end

