# /var/mail/:saved is built directly by the rootdirs target in
# /usr/src/Makefile because of the colon in its name.

# macros for symbolic links
SYM.DIRS= \
	/bin \
	/dev/stdin \
	/dev/stdout \
	/dev/stderr \
	/etc/lib/ld.so.1 \
	/etc/lib/libdl.so.1 \
	/etc/lib/nss_files.so.1 \
	/etc/log \
	/lib/32 \
	/lib/crypto/32 \
	/lib/secure/32 \
	/usr/adm \
	/usr/spool \
	/usr/lib/tmac \
	/usr/ccs/lib/link_audit \
	/usr/news \
	/usr/preserve \
	/usr/lib/32 \
	/usr/lib/cron \
	/usr/lib/elfedit/32 \
	/usr/lib/lwp/32 \
	/usr/lib/link_audit/32 \
	/usr/lib/secure/32 \
	/usr/mail \
	/usr/man \
	/usr/pub \
	/usr/src \
	/usr/tmp \
	/usr/ucblib/32 \
	/var/ld/32

i386_SYM.DIRS64= \
	/usr/lib/brand/lx/64

sparc_SYM.DIRS64=

