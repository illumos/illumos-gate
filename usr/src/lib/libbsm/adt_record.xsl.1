</xsl:template>

<xsl:template match="cmd">
	<BR/>
	<I>CMD </I>
	<xsl:apply-templates/>
</xsl:template>

<xsl:template match="argv">
	<BR/>
	<I>argv: </I>   <xsl:value-of select="."/>
</xsl:template>

<xsl:template match="arge">
	<BR/>
