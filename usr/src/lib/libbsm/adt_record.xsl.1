</xsl:template>

<xsl:template match="fmri">
	<BR/>
	<I>FMRI: </I>   <xsl:value-of select="."/>
</xsl:template>

<xsl:template match="user">
	<BR/>
	<I>USER </I>
	<I> uid: </I><xsl:value-of select="@uid"/>
