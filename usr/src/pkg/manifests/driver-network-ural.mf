<include global_zone_only_component>
set name=pkg.fmri value=pkg:/driver/network/ural@$(PKGVERS)
set name=pkg.description value="Ralink RT2500USB 802.11b/g Wireless Driver"
set name=pkg.summary value="Ralink RT2500USB 802.11b/g Wireless Driver"
set name=info.classification \
    value=org.opensolaris.category.2008:Drivers/Networking
set name=variant.arch value=i386
dir path=kernel group=sys
dir path=kernel/drv group=sys
dir path=kernel/drv/$(ARCH64) group=sys
dir path=usr/share/man
dir path=usr/share/man/man7d
driver name=ural clone_perms="ural 0666 root sys" perms="* 0666 root sys" \
    alias=usb1044,8007 \
    alias=usb13b1,d \
    alias=usb2001,3c00 \
    alias=usb411,67
file path=kernel/drv/$(ARCH64)/ural group=sys
file path=usr/share/man/man7d/ural.7d
legacy pkg=SUNWural desc="Ralink RT2500USB 802.11b/g Wireless Driver" \
    name="Ralink RT2500USB 802.11b/g Wireless Driver"
license cr_Sun license=cr_Sun
license usr/src/uts/common/io/ural/THIRDPARTYLICENSE \
    license=usr/src/uts/common/io/ural/THIRDPARTYLICENSE
