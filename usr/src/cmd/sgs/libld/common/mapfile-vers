SUNWprivate_4.4 {
	global:
		dbg_print  = NODIRECT;	# interposed - ld(1) and ld.so.1(1)

		ld_getopt;

		ld32_main;
		ld64_main;

		ld32_create_outfile;
		ld64_create_outfile;
		ld32_ent_setup;
		ld64_ent_setup;
		ld32_init_strings;
		ld64_init_strings;
		ld32_init_target;
		ld64_init_target;
		ld32_make_sections;
		ld64_make_sections;
		ld32_ofl_cleanup;
		ld64_ofl_cleanup;
		ld32_process_open;
		ld64_process_open;
		ld32_reloc_init;
		ld64_reloc_init;
		ld32_reloc_process;
		ld64_reloc_process;
		ld32_sym_validate;
		ld64_sym_validate;
		ld32_update_outfile;
		ld64_update_outfile;
	local:
		*;
};

# The following functions must be provided by our caller, or one of the
# callers dependencies.

{
	global:
		eprintf =	FUNCTION extern;
};
