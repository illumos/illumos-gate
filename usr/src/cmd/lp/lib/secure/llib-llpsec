#pragma ident	"%Z%%M%	%I%	%E% SMI"

#include "sys/types.h"

typedef struct SECURE {
    uid_t	uid;
    gid_t	gid;
    off_t	size;
    time_t	date;
    char	*system;
    char	*user;
    char	*req_id;
}			SECURE;


SECURE *	getsecure ( char * );
int		putsecure ( char *, SECURE * );
int		rmsecure (char *);
void		freesecure ( SECURE * );

