	cr=^M, cud1=^J, ind=^J, bel=^G, cols#80, lines#24, cub1=^H$<10>,
	smacs=^z2, rmacs=^z3, cup=\015\036%r%p1%' '%+%c%p2%' '%+%c$<6>,
	el=\035$<6>, home=\031$<50>, smso=^z3, rmso=^z2,
